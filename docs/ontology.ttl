#################################################################
#    Rules
#################################################################

<urn:swrl:var#passenger> rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

<urn:swrl:var#country> rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

<urn:swrl:var#review> rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

<urn:swrl:var#rating> rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

<urn:swrl:var#body> rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

[ rdf:type <http://www.w3.org/2003/11/swrl#Imp> ;
   <http://www.w3.org/2003/11/swrl#body> [ rdf:type <http://www.w3.org/2003/11/swrl#AtomList> ;
                                           rdf:first [ rdf:type <http://www.w3.org/2003/11/swrl#DatavaluedPropertyAtom> ;
                                                       <http://www.w3.org/2003/11/swrl#propertyPredicate> :Rating ;
                                                       <http://www.w3.org/2003/11/swrl#argument1> <urn:swrl:var#review> ;
                                                       <http://www.w3.org/2003/11/swrl#argument2> <urn:swrl:var#rating>
                                                     ] ;
                                           rdf:rest [ rdf:type <http://www.w3.org/2003/11/swrl#AtomList> ;
                                                      rdf:first [ rdf:type <http://www.w3.org/2003/11/swrl#BuiltinAtom> ;
                                                                  <http://www.w3.org/2003/11/swrl#builtin> <http://www.w3.org/2003/11/swrlb#contains> ;
