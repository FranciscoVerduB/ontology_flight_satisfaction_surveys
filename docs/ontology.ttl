@prefix : <http://www.semanticweb.org/francisco_jose_verdu/ontologies/2024/3/flight_satisfaction_surveys#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/francisco_jose_verdu/ontologies/2024/3/flight_satisfaction_surveys#> .

<http://www.semanticweb.org/francisco_jose_verdu/ontologies/2024/3/flight_satisfaction_surveys> rdf:type owl:Ontology ;
                                                                                                 rdfs:comment "Its objective is to model and represent the structure, relationships and concepts related to flight satisfaction surveys"@en ;
                                                                                                 rdfs:label "fligh_satisfaction_surveys"@en .

#################################################################
#    Annotation properties
#################################################################

###  http://www.w3.org/2000/01/rdf-schema#sameAs
rdfs:sameAs rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/francisco_jose_verdu/ontologies/2024/3/flight_satisfaction_surveys#Aircraft
:Aircraft rdf:type owl:ObjectProperty ;
          rdfs:domain :Aircraft .


###  http://www.semanticweb.org/francisco_jose_verdu/ontologies/2024/3/flight_satisfaction_surveys#Airline
:Airline rdf:type owl:ObjectProperty ;
         rdfs:domain :Review .


###  http://www.semanticweb.org/francisco_jose_verdu/ontologies/2024/3/flight_satisfaction_surveys#Airport
:Airport rdf:type owl:ObjectProperty ;
         rdfs:domain :Airport .


###  http://www.semanticweb.org/francisco_jose_verdu/ontologies/2024/3/flight_satisfaction_surveys#Country
:Country rdf:type owl:ObjectProperty ;
         rdfs:domain :Country .


###  http://www.semanticweb.org/francisco_jose_verdu/ontologies/2024/3/flight_satisfaction_surveys#FlighSeat
:FlighSeat rdf:type owl:ObjectProperty ;
           rdfs:domain :FlightRoute ;
           rdfs:label "FlighSeat"@en .


###  http://www.semanticweb.org/francisco_jose_verdu/ontologies/2024/3/flight_satisfaction_surveys#FlightRoute
:FlightRoute rdf:type owl:ObjectProperty ;
             rdfs:domain :FlightRoute .


###  http://www.semanticweb.org/francisco_jose_verdu/ontologies/2024/3/flight_satisfaction_surveys#Location
:Location rdf:type owl:ObjectProperty ;
          rdfs:domain :Location .


###  http://www.semanticweb.org/francisco_jose_verdu/ontologies/2024/3/flight_satisfaction_surveys#Passenger
:Passenger rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :Review ;
           rdfs:domain :Passenger .


###  http://www.semanticweb.org/francisco_jose_verdu/ontologies/2024/3/flight_satisfaction_surveys#Review
:Review rdf:type owl:ObjectProperty ;
        rdfs:domain :Review .


###  http://www.semanticweb.org/francisco_jose_verdu/ontologies/2024/3/flight_satisfaction_surveys#Trip
:Trip rdf:type owl:ObjectProperty ;
      rdfs:domain :Trip .


###  http://www.semanticweb.org/francisco_jose_verdu/ontologies/2024/3/flight_satisfaction_surveys#TripReason
:TripReason rdf:type owl:ObjectProperty ;
            rdfs:domain :TripReason .


###  http://www.semanticweb.org/francisco_jose_verdu/ontologies/2024/3/flight_satisfaction_surveys#containsAirport
:containsAirport rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :Location ;
                 owl:inverseOf :isLocalizedIn ;
                 rdfs:domain :Location ;
                 rdfs:range :Airport ;
                 rdfs:comment "Indicates that an entity contains an airport."@en ;
                 rdfs:label "containsAirport"@en .


###  http://www.semanticweb.org/francisco_jose_verdu/ontologies/2024/3/flight_satisfaction_surveys#fillingByPassenger
:fillingByPassenger rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf :Review ;
                    owl:inverseOf :providesReview ;
                    rdf:type owl:FunctionalProperty ;
                    rdfs:domain :Review ;
                    rdfs:range :Passenger ;
                    rdfs:comment "Indicates the passenger who completed the survey"@en ;
                    rdfs:label "fillingByPassenger"@en .


###  http://www.semanticweb.org/francisco_jose_verdu/ontologies/2024/3/flight_satisfaction_surveys#followsFlightRoute
:followsFlightRoute rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf :Trip ;
                    rdf:type owl:FunctionalProperty ;
                    rdfs:domain :Trip ;
                    rdfs:range :FlightRoute ;
                    rdfs:comment "Indicates the flight route followed during a trip."@en ;
                    rdfs:label "hasFlightRoute"@en .


###  http://www.semanticweb.org/francisco_jose_verdu/ontologies/2024/3/flight_satisfaction_surveys#hasAircraft
:hasAircraft rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :Trip ;
             rdf:type owl:FunctionalProperty ;
             rdfs:domain :Trip ;
             rdfs:range :Aircraft ;
             rdfs:comment "Indicates the aircraft used for a specific trip."@en ;
             rdfs:label "usesAircraft"@en .


###  http://www.semanticweb.org/francisco_jose_verdu/ontologies/2024/3/flight_satisfaction_surveys#hasAirport
:hasAirport rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :Country ;
            rdf:type owl:TransitiveProperty ;
            rdfs:domain :Country ;
            rdfs:range :Airport ;
            rdfs:comment "Indicates the airport(s) located in a country."@en ;
            rdfs:label "hasAirport"@en .


###  http://www.semanticweb.org/francisco_jose_verdu/ontologies/2024/3/flight_satisfaction_surveys#hasArrivalAirport
:hasArrivalAirport rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf :FlightRoute ;
                   rdf:type owl:FunctionalProperty ;
                   rdfs:domain :FlightRoute ;
                   rdfs:range :Airport ;
                   rdfs:comment "Indicates the arrival airport for a flight route."@en ;
                   rdfs:label "hasArrivalAirport"@en .


###  http://www.semanticweb.org/francisco_jose_verdu/ontologies/2024/3/flight_satisfaction_surveys#hasCountry
:hasCountry rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :Location ;
            owl:inverseOf :isLocatedIn ;
            rdf:type owl:FunctionalProperty ;
            rdfs:domain :Location ;
            rdfs:range :Country ;
            rdfs:comment "Indicates the country associated with an entity."@en ;
            rdfs:label "hasCountry"@en .


###  http://www.semanticweb.org/francisco_jose_verdu/ontologies/2024/3/flight_satisfaction_surveys#hasDepartureAirport
:hasDepartureAirport rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf :FlightRoute ;
                     rdf:type owl:FunctionalProperty ;
                     rdfs:domain :FlightRoute ;
                     rdfs:range :Airport ;
                     rdfs:comment "Indicates the departure airport for a flight route."@en ;
                     rdfs:label "hasDepartureAirport"@en .


###  http://www.semanticweb.org/francisco_jose_verdu/ontologies/2024/3/flight_satisfaction_surveys#hasFlighSeat
:hasFlighSeat rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :Trip ;
              owl:inverseOf :hasTrip ;
              rdfs:domain :Trip ;
              rdfs:range :FlightSeat ;
              rdfs:label "hasFlightSeat"@en .


###  http://www.semanticweb.org/francisco_jose_verdu/ontologies/2024/3/flight_satisfaction_surveys#hasOriginCountry
:hasOriginCountry rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf :Passenger ;
                  rdf:type owl:FunctionalProperty ;
                  rdfs:domain :Passenger ;
                  rdfs:range :Country ;
                  rdfs:comment "Indicates the country of origin of a passenger."@en ;
                  rdfs:label "Passenger"@en .


###  http://www.semanticweb.org/francisco_jose_verdu/ontologies/2024/3/flight_satisfaction_surveys#hasPassenger
:hasPassenger rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :Trip ;
              owl:inverseOf :takesTrip ;
              rdf:type owl:FunctionalProperty ;
              rdfs:domain :Trip ;
              rdfs:range :Passenger ;
              rdfs:comment "Indicates the passenger(s) associated with a trip."@en ;
              rdfs:label "hasPassenger"@en .


###  http://www.semanticweb.org/francisco_jose_verdu/ontologies/2024/3/flight_satisfaction_surveys#hasTrip
:hasTrip rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf :FlighSeat ;
         rdfs:domain :FlightRoute ;
         rdfs:range :Trip ;
         rdfs:comment "Indicates the flight seat associated with a trip."@en ;
         rdfs:label "hasTrip"@en .


###  http://www.semanticweb.org/francisco_jose_verdu/ontologies/2024/3/flight_satisfaction_surveys#hasTripReason
:hasTripReason rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf :Review ;
               owl:inverseOf :isReasonForTrip ;
               rdf:type owl:FunctionalProperty ;
               rdfs:domain :Review ;
               rdfs:range :TripReason ;
               rdfs:comment "Indicates the trip reason associated with a trip."@en ;
               rdfs:label "hasTripReason"@en .


###  http://www.semanticweb.org/francisco_jose_verdu/ontologies/2024/3/flight_satisfaction_surveys#isLocalizedIn
:isLocalizedIn rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf :Airport ;
               rdf:type owl:FunctionalProperty ;
               rdfs:domain :Airport ;
               rdfs:range :Location ;
               rdfs:comment "Indicates that an entity is located in another place."@en ;
               rdfs:label "isUbicatedIn"@en .


###  http://www.semanticweb.org/francisco_jose_verdu/ontologies/2024/3/flight_satisfaction_surveys#isLocatedIn
:isLocatedIn rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :Country ;
             rdf:type owl:FunctionalProperty ;
             rdfs:domain :Country ;
             rdfs:range :Location ;
             rdfs:comment "Indicates the location of an airport or country."@en ;
             rdfs:label "isLocateIn"@en .


###  http://www.semanticweb.org/francisco_jose_verdu/ontologies/2024/3/flight_satisfaction_surveys#isOwnedBy
:isOwnedBy rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :Aircraft ;
           owl:inverseOf :usesAircraft ;
           rdfs:domain :Aircraft ;
           rdfs:range :Airline ;
           rdfs:comment "Indicates that an entity is owned by another."@en ;
           rdfs:label "isOwnedBy"@en .


###  http://www.semanticweb.org/francisco_jose_verdu/ontologies/2024/3/flight_satisfaction_surveys#isReasonForTrip
:isReasonForTrip rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :TripReason ;
                 rdfs:domain :TripReason ;
                 rdfs:range :Review ;
                 rdfs:comment "Indicates that a trip reason is the reason for a trip."@en ;
                 rdfs:label "isTripReasonFor"@en .


###  http://www.semanticweb.org/francisco_jose_verdu/ontologies/2024/3/flight_satisfaction_surveys#operatesFromAirport
:operatesFromAirport rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf :Airline ;
                     owl:inverseOf :serversAirline ;
                     rdfs:domain :Airline ;
                     rdfs:range :Airport ;
                     rdfs:comment "Indicates the airport(s) from which an airline operates."@en ;
                     rdfs:label "operatesFromAirport"@en .


###  http://www.semanticweb.org/francisco_jose_verdu/ontologies/2024/3/flight_satisfaction_surveys#providesReview
:providesReview rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :Passenger ;
                rdfs:domain :Passenger ;
                rdfs:range :Review ;
                rdfs:comment "Indicates that a passenger provides a review."@en ;
                rdfs:label "providesReview"@en .


###  http://www.semanticweb.org/francisco_jose_verdu/ontologies/2024/3/flight_satisfaction_surveys#serversAirline
:serversAirline rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :Airport ;
                rdfs:domain :Airport ;
                rdfs:range :Airline ;
                rdfs:comment "Indicates that an airport serves the operations of an airline."@en ;
                rdfs:label "serversAirline"@en .


###  http://www.semanticweb.org/francisco_jose_verdu/ontologies/2024/3/flight_satisfaction_surveys#takesTrip
:takesTrip rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :Passenger ;
           rdf:type owl:FunctionalProperty ;
           rdfs:domain :Passenger ;
           rdfs:range :Trip ;
           rdfs:comment "Indicates that a passenger takes part in a trip."@en ;
           rdfs:label "takesTrip"@en .


###  http://www.semanticweb.org/francisco_jose_verdu/ontologies/2024/3/flight_satisfaction_surveys#usesAircraft
:usesAircraft rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :Airline ;
              rdfs:domain :Airline ;
              rdfs:range :Aircraft ;
              rdfs:comment "Indicates the aircraft operated by an airline."@en ;
              rdfs:label "hasAircraft"@en .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/francisco_jose_verdu/ontologies/2024/3/flight_satisfaction_surveys#CountryName
:CountryName rdf:type owl:DatatypeProperty ;
             rdfs:domain :Country ;
             rdfs:range xsd:string ;
             rdfs:label "CountryName"@en .


###  http://www.semanticweb.org/francisco_jose_verdu/ontologies/2024/3/flight_satisfaction_surveys#DateFlown
:DateFlown rdf:type owl:DatatypeProperty ,
                    owl:FunctionalProperty ;
           rdfs:domain :Trip ;
           rdfs:range xsd:dateTime ;
           rdfs:comment "Denotes the date when the trip or journey took place or was undertaken."@en ;
           rdfs:label "DateFlown"@en .


###  http://www.semanticweb.org/francisco_jose_verdu/ontologies/2024/3/flight_satisfaction_surveys#DatePublished
:DatePublished rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf :ReviewData ;
               rdf:type owl:FunctionalProperty ;
               rdfs:domain :Review ;
               rdfs:range xsd:dateTime ;
               rdfs:comment "Represents the date when the review or opinion was published."@en ;
               rdfs:label "DatePublished"@en .


###  http://www.semanticweb.org/francisco_jose_verdu/ontologies/2024/3/flight_satisfaction_surveys#Rating
:Rating rdf:type owl:DatatypeProperty ;
        rdfs:subPropertyOf :TripFeedback ;
        rdf:type owl:FunctionalProperty ;
        rdfs:domain :Review ;
        rdfs:range [ rdf:type rdfs:Datatype ;
                     owl:intersectionOf ( [ rdf:type rdfs:Datatype ;
                                            owl:onDatatype xsd:int ;
                                            owl:withRestrictions ( [ xsd:minExclusive "0"^^xsd:int
                                                                   ]
                                                                 )
                                          ]
                                          [ rdf:type rdfs:Datatype ;
                                            owl:onDatatype xsd:int ;
                                            owl:withRestrictions ( [ xsd:maxExclusive "11"^^xsd:int
                                                                   ]
                                                                 )
                                          ]
                                        )
                   ] ;
        rdfs:comment "Quantifies the evaluation or score given to the subject of the review or opinion."@en ;
        rdfs:label "Rating"@en .


###  http://www.semanticweb.org/francisco_jose_verdu/ontologies/2024/3/flight_satisfaction_surveys#Recomended
:Recomended rdf:type owl:DatatypeProperty ;
            rdfs:subPropertyOf :TripFeedback ;
            rdf:type owl:FunctionalProperty ;
            rdfs:domain :Review ;
            rdfs:range xsd:boolean ;
            rdfs:comment "Specifies whether the subject of the review or opinion is recommended or not."@en ;
            rdfs:label "Recomended"@en .


###  http://www.semanticweb.org/francisco_jose_verdu/ontologies/2024/3/flight_satisfaction_surveys#ReviewBody
:ReviewBody rdf:type owl:DatatypeProperty ;
            rdfs:subPropertyOf :ReviewData ;
            rdf:type owl:FunctionalProperty ;
            rdfs:domain :Review ;
            rdfs:range xsd:string ;
            rdfs:comment "Indicates the main content or body of the review or opinion."@en ;
            rdfs:label "ReviewBody"@en .


###  http://www.semanticweb.org/francisco_jose_verdu/ontologies/2024/3/flight_satisfaction_surveys#ReviewData
:ReviewData rdf:type owl:DatatypeProperty ;
            rdfs:domain :Review ;
            rdfs:label "ReviewData"@en .


###  http://www.semanticweb.org/francisco_jose_verdu/ontologies/2024/3/flight_satisfaction_surveys#ReviewTitle
:ReviewTitle rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf :ReviewData ;
             rdf:type owl:FunctionalProperty ;
             rdfs:domain :Review ;
             rdfs:range xsd:string ;
             rdfs:comment "Denotes the title or headline of the review or opinion."@en ;
             rdfs:label "ReviewTitle"@en .


###  http://www.semanticweb.org/francisco_jose_verdu/ontologies/2024/3/flight_satisfaction_surveys#TripFeedback
:TripFeedback rdf:type owl:DatatypeProperty ;
              rdfs:domain :Review ;
              rdfs:comment "Represents the feedback or commentary provided regarding a trip"@en ;
              rdfs:label "TripFeedback"@en .


###  http://www.semanticweb.org/francisco_jose_verdu/ontologies/2024/3/flight_satisfaction_surveys#Verified
:Verified rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf :TripFeedback ;
          rdf:type owl:FunctionalProperty ;
          rdfs:domain :Review ;
          rdfs:range xsd:boolean ;
          rdfs:comment "Indicates whether the review or opinion has been verified or authenticated."@en ;
          rdfs:label "Verified"@en .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/francisco_jose_verdu/ontologies/2024/3/flight_satisfaction_surveys#Aircraft
:Aircraft rdf:type owl:Class ;
          rdfs:subClassOf [ rdf:type owl:Restriction ;
                            owl:onProperty :hasCountry ;
                            owl:allValuesFrom :Country
                          ] .


###  http://www.semanticweb.org/francisco_jose_verdu/ontologies/2024/3/flight_satisfaction_surveys#Airline
:Airline rdf:type owl:Class ;
         owl:equivalentClass [ rdf:type owl:Class ;
                               owl:oneOf ( :Airline_Qatar
                                           :Airline_Ryanair
                                         )
                             ] .


###  http://www.semanticweb.org/francisco_jose_verdu/ontologies/2024/3/flight_satisfaction_surveys#Airport
:Airport rdf:type owl:Class .


###  http://www.semanticweb.org/francisco_jose_verdu/ontologies/2024/3/flight_satisfaction_surveys#Business
:Business rdf:type owl:Class ;
          rdfs:subClassOf :FlightSeat ;
          rdfs:comment "Representing a business class seat on a flight"@en ;
          rdfs:label "Business"@en .


###  http://www.semanticweb.org/francisco_jose_verdu/ontologies/2024/3/flight_satisfaction_surveys#BusinessTripReason
:BusinessTripReason rdf:type owl:Class ;
                    rdfs:subClassOf :TripReason ;
                    rdfs:comment "Representing a trip undertaken for business purposes"@en ;
                    rdfs:label "BusinessTripReason"@en .


###  http://www.semanticweb.org/francisco_jose_verdu/ontologies/2024/3/flight_satisfaction_surveys#CanadianPassenger
:CanadianPassenger rdf:type owl:Class ;
                   rdfs:subClassOf :Passenger ;
                   rdfs:comment "A subtype of the general class \"Passenger\", specifically denoting individuals who hold Canadian nationality. Canadian passengers are individuals from Canada who engage in air travel for various purposes such as business, leisure, or family visits."@en ;
                   rdfs:isDefinedBy "http://dbpedia.org/resource/Canadian" ;
                   rdfs:label "CanadianPassenger"@en ;
                   rdfs:sameAs "http://dbpedia.org/resource/Canadian"@en .


###  http://www.semanticweb.org/francisco_jose_verdu/ontologies/2024/3/flight_satisfaction_surveys#Country
:Country rdf:type owl:Class .


###  http://www.semanticweb.org/francisco_jose_verdu/ontologies/2024/3/flight_satisfaction_surveys#CoupleTripReason
:CoupleTripReason rdf:type owl:Class ;
                  rdfs:subClassOf :LeisureTripReason ;
                  rdfs:comment "Representing a leisure trip undertaken by a couple"@en ;
                  rdfs:label "CoupleTripReason"@en .


###  http://www.semanticweb.org/francisco_jose_verdu/ontologies/2024/3/flight_satisfaction_surveys#Economy
:Economy rdf:type owl:Class ;
         rdfs:subClassOf :FlightSeat ;
         rdfs:comment "Representing a Economy class seat on a flight"@en ;
         rdfs:label "Economy"@en .


###  http://www.semanticweb.org/francisco_jose_verdu/ontologies/2024/3/flight_satisfaction_surveys#FamilyTripReason
:FamilyTripReason rdf:type owl:Class ;
                  rdfs:subClassOf :LeisureTripReason ;
                  rdfs:comment "Representing a leisure trip undertaken by a family"@en ;
                  rdfs:label "FamilyTripReason"@en .


###  http://www.semanticweb.org/francisco_jose_verdu/ontologies/2024/3/flight_satisfaction_surveys#FirstClass
:FirstClass rdf:type owl:Class ;
            rdfs:subClassOf :FlightSeat ;
            rdfs:comment "Representing a first class seat on a flight"@en ;
            rdfs:label "FirstClass"@en .


###  http://www.semanticweb.org/francisco_jose_verdu/ontologies/2024/3/flight_satisfaction_surveys#FlightRoute
:FlightRoute rdf:type owl:Class ;
             rdfs:subClassOf [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                      owl:onProperty :hasArrivalAirport ;
                                                      owl:allValuesFrom :FlightRoute
                                                    ]
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty :hasDepartureAirport ;
                                                      owl:allValuesFrom :FlightRoute
                                                    ]
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty :hasArrivalAirport ;
                                                      owl:minCardinality "1"^^xsd:nonNegativeInteger
                                                    ]
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty :hasDepartureAirport ;
                                                      owl:minCardinality "1"^^xsd:nonNegativeInteger
                                                    ]
                                                  ) ;
                               rdf:type owl:Class
                             ] .


###  http://www.semanticweb.org/francisco_jose_verdu/ontologies/2024/3/flight_satisfaction_surveys#FlightSeat
:FlightSeat rdf:type owl:Class ;
            rdfs:comment "Representing a seat on a flight"@en ;
            rdfs:label "FlightSeat"@en .


###  http://www.semanticweb.org/francisco_jose_verdu/ontologies/2024/3/flight_satisfaction_surveys#GermanPassenger
:GermanPassenger rdf:type owl:Class ;
                 rdfs:subClassOf :Passenger ;
                 owl:disjointWith :SpanishPassenger ;
                 rdfs:comment "A subtype of the general class \"Passenger\", specifically denoting individuals who hold German nationality. German passengers are citizens of Germany who travel by air, contributing to international air traffic."@en ;
                 rdfs:isDefinedBy "http://dbpedia.org/resource/German" ;
                 rdfs:label "GermanPassenger"@en ;
                 rdfs:sameAs "http://dbpedia.org/resource/German" .


###  http://www.semanticweb.org/francisco_jose_verdu/ontologies/2024/3/flight_satisfaction_surveys#GoodReview
:GoodReview rdf:type owl:Class ;
            rdfs:subClassOf :Review ;
            owl:disjointWith :NegativeReview ;
            rdfs:comment "A subtype of Review, representing feedback that emphasizes positive aspects of the travel experience. Good reviews may commend excellent service, comfortable seating, or timely arrivals, serving as testimonials of satisfaction"@en ;
            rdfs:label "GoodReview"@en .


###  http://www.semanticweb.org/francisco_jose_verdu/ontologies/2024/3/flight_satisfaction_surveys#LeisureTripReason
:LeisureTripReason rdf:type owl:Class ;
                   rdfs:subClassOf :TripReason ;
                   rdfs:comment "Representing a trip undertaken for leisure or recreational purposes"@en ;
                   rdfs:label "LeisureTripReason"@en .


###  http://www.semanticweb.org/francisco_jose_verdu/ontologies/2024/3/flight_satisfaction_surveys#Location
:Location rdf:type owl:Class .


###  http://www.semanticweb.org/francisco_jose_verdu/ontologies/2024/3/flight_satisfaction_surveys#NegativeReview
:NegativeReview rdf:type owl:Class ;
                rdfs:subClassOf :Review ;
                rdfs:comment "A subtype of Review, specifically indicating feedback that highlights negative aspects of the travel experience. Negative reviews may include complaints about flight delays, poor service, or discomfort, providing insights into areas for improvement."@en ;
                rdfs:label "NegativeReview"@en .


###  http://www.semanticweb.org/francisco_jose_verdu/ontologies/2024/3/flight_satisfaction_surveys#Passenger
