###  http://www.semanticweb.org/francisco_jose_verdu/ontologies/2024/3/flight_satisfaction_surveys#Traveler
:Traveler rdf:type owl:Class ;
          owl:equivalentClass [ owl:intersectionOf ( :Passenger
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty :hasTrip ;
                                                       owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                       owl:onClass :Trip
                                                     ]
                                                   ) ;
                                rdf:type owl:Class
                              ] ;
          rdfs:subClassOf :Passenger ;
          rdfs:comment "A general class encompassing all individuals who engage in travel activities, including air travel. Travelers can be of any nationality and may embark on journeys for various reasons, such as tourism, business, or personal commitments"@en ;
          rdfs:label "Traveler"@en .


